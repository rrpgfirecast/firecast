	-- ve se acertou e aonde acertou o ataque nas batalhas
	local function BatalhaAtaque(porcentagem , dificuldade, atributo)
	
		if atributo == nil then atributo = "---"; end;
		if sheet.NOMEdoPERSONAGEM == nil then sheet.NOMEdoPERSONAGEM = "0";end;
		
		
		
		local roll = rrpg.interpretarRolagem("1d100");
		local roll2 = rrpg.interpretarRolagem("1d100");
		roll:rolarLocalmente();
		roll2:rolarLocalmente();
		
		local chat = rrpg.getMesaDe(sheet).chat;
		chat:enviarMensagem(sheet.NOMEdoPERSONAGEM .. " usou ATAQUE: " .. atributo .. ", com dificuldade " .. dificuldade .. " e com " .. porcentagem .. "% de chance de acerto."  );
		
		
		mirafunc()
		
		if roll.resultado <= porcentagem then 
			--sucesso	
		
			
			if roll.resultado <= 5 then
				chat:enviarMensagem("Resultado: " .. roll.resultado .. ", SUCESSO CRÍTICO!!! DOBRO DO DANO!" );
			else
				chat:enviarMensagem("Resultado: " .. roll.resultado .. ", SUCESSO!" );
			end;
			
			local lugar = "a";

			
			if roll2.resultado <= 30 then
				lugar = "Torso"
				 sheet.mira2 = "/imagens/mira.png";
			else
				if roll2.resultado <= 40 then
					lugar = "Cabeça"
					 sheet.mira1 = "/imagens/mira.png";
				else
					if roll2.resultado <= 50 then
						lugar = "Perna Direita"
						sheet.mira3 = "/imagens/mira.png";
					else
						if roll2.resultado <= 60 then
							lugar = "Perna Esquerda"
							sheet.mira4 = "/imagens/mira.png";
						else
							if roll2.resultado <= 70 then
								lugar = "Braço Direito"
								sheet.mira5 = "/imagens/mira.png";
							else
								if roll2.resultado <= 80 then
									lugar = "Braço Esquerdo"
									sheet.mira6 = "/imagens/mira.png";
								else
									if roll2.resultado <= 85 then
										lugar = "Mão Direita"
										sheet.mira7 = "/imagens/mira.png";
									else
										if roll2.resultado <= 90 then
											lugar = "Mão Esquerda"
											sheet.mira8 = "/imagens/mira.png";
										else
											if roll2.resultado <= 95 then
												lugar = "Pé Direito"
												sheet.mira9 = "/imagens/mira.png";
											else
												lugar = "Pé Esquerdo"
												sheet.mira10 = "/imagens/mira.png";
											end;
										end;
									end;
								end;
							end;	
						end;
					end;
				end;
			end;

			chat:enviarMensagem("Lugar acertado: " .. lugar );
			
		
		else
			--falha
			sheet.mira11 = "/imagens/mira.png";
			if roll.resultado >= 95 then
				chat:enviarMensagem("Resultado: " .. roll.resultado .. ", FALHA CRÍTICA!!!" );
			else
				chat:enviarMensagem("Resultado: " .. roll.resultado .. ", FALHA!" );
			end;
		
		
		end;
		
		return ;
		
		
		
	
		
		
	end;
	
	
	
	
