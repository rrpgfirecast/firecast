	--rolar dados para dano da batalha
	local function Rolardadosparadano(Nome,DanoTotal,DanoFOR,DanoCRITICO)
		local roll = rrpg.interpretarRolagem(DanoTotal);
		roll:rolarLocalmente();
		if Nome == nil then Nome = "0";end;
		if DanoTotal == nil then DanoTotal = "0";end;
		local chat = rrpg.getMesaDe(sheet).chat;
		if sheet.NOMEdoPERSONAGEM == nil then sheet.NOMEdoPERSONAGEM = "0";end;
		
		if DanoFOR == true then
			if DanoCRITICO == true then
				chat:enviarMensagem(sheet.NOMEdoPERSONAGEM .. " usou " .. Nome .. " com dano " .. DanoTotal .. "! Com Crítico e Bônus de FOR." );
			else
				chat:enviarMensagem(sheet.NOMEdoPERSONAGEM .. " usou " .. Nome .. " com dano " .. DanoTotal .. "! Com Bônus de FOR." );
			end;
		else
			if DanoCRITICO == true then
				chat:enviarMensagem(sheet.NOMEdoPERSONAGEM .. " usou " .. Nome .. " com dano " .. DanoTotal .. "! Com Crítico." );
			else
				chat:enviarMensagem(sheet.NOMEdoPERSONAGEM .. " usou " .. Nome .. " com dano " .. DanoTotal .. "!" );
			end;
		end;
		
		chat:enviarMensagem("Dano total: " .. roll.resultado .. "!" );
	
	end;
	
	--Batalha calculo do dano total
	
	local function Danototalcalc(Ndados,Tdados,Mdado,DanoFOR,DanoCRITICO)

	
		if Ndados == nil then Ndados = 0;end;
		if Tdados == nil then Tdados = 0;end;
		if Mdado == nil then Mdado = 0;end;
	
		-- calculando o bonus do dano da for
		if DanoFOR == true then
			if sheet.ATRtotal2 < 30 then
				Mdado = Mdado + toint((sheet.ATRtotal2 - 30)/5);
			end;
			if sheet.ATRtotal2 > 50 then
			
				Mdado = Mdado + toint((sheet.ATRtotal2 - 50)/5);
			end;
		end;
		
		--multiplicando Numero de dados e o modificador pelo crítico
		if DanoCRITICO == true then
			Ndados = toint(Ndados*2)
			Mdado = toint(Mdado*2)
		end;
	

		--enviando dano total
		if Mdado >=0 then
			return Ndados .. "d" .. Tdados .. "+" .. Mdado ;
		else
			return Ndados .. "d" .. Tdados .. "" .. Mdado ;
		end;
		--lembrar quando modificar a FOR mudar o danototalcalc tb
	end;
	
	--ve se esquivou
	local function BatalhaDefesa(porcentagem , dificuldade, atributo)
		if sheet.NOMEdoPERSONAGEM == nil then sheet.NOMEdoPERSONAGEM = "0";end;
		if atributo == nil then atributo = "---"; end;
		local roll = rrpg.interpretarRolagem("1d100");
		roll:rolarLocalmente();
		
		
		local chat = rrpg.getMesaDe(sheet).chat;
		chat:enviarMensagem(sheet.NOMEdoPERSONAGEM .. " usou DEFESA: " .. atributo .. " contra o ataque com dificuldade " .. dificuldade .. " e com " .. porcentagem .. "% de chance de sucesso."  );
		
		
		
		
		if roll.resultado <= porcentagem then 
			--sucesso	
			if roll.resultado <= 5 then
				chat:enviarMensagem("Resultado: " .. roll.resultado .. ", SUCESSO CRÍTICO!!!" );
			else
				chat:enviarMensagem("Resultado: " .. roll.resultado .. ", SUCESSO!" );
			end;
			
		
		
		else
			--falha
			if roll.resultado >= 95 then
				chat:enviarMensagem("Resultado: " .. roll.resultado .. ", FALHA CRÍTICA!!!" );
			else
				chat:enviarMensagem("Resultado: " .. roll.resultado .. ", FALHA!" );
			end;
		
		
		end;
		
		return ;

	
	end;
	
	
