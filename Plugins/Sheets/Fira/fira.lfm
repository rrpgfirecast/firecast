	--levantarComEquipamentos
	
	local function levantarComEquipamentosfunc(levantar, carregando, carregar);
	
		if levantar == nil then	return 0;end;
		if carregando == nil then	return 0;end;
		if carregar == nil then	return 0;end;
		
		return toint(levantar - carregando);
		
	end;
	
	
	-- se consegue andar
	
	local function MensagemAndarfunc(x)
		if x == nil then
			x=0;
		end;
		
		
		if x >= 100 then
			return "+100%-> NÃO CONSEGUE ANDAR!" ;
		else	
			if x >= 90 then
				return "+90%-> CONSEGUE ANDAR MUITO LIMITADAMENTE!";
			else
				if x >= 75 then
					return "+75%-> Consegue andar limitadamente!";
				else
					if x >= 50 then
						return  "+50%-> Consegue andar um pouco limitado!" ;
					else
						return  "Menos de 50%-> Anda normalmente!" ;
					end;
				end;
			end;
		end;
	end;
	
	
	--retorna a % 
	local function Barporcentagem(NumEqX,NumEq100)
		if NumEq100 == nil then
			return 0;
		end;
		if NumEqX == nil then
			return 0;
		end;
	
		return toint((NumEqX*100)/NumEq100) ;
		
	end;
	
