        <tab title="Recursos">      
        <scrollBox align="client">
        <layout align="none" height="35" width="1329" top="15" left="5" frameStyle="/frames/FrameRetangular.xml" 
        margins="{left=30, top=15, right=10, bottom=10}">
                
        <label text="Equipamentos e pertences / Dinheiro e Recursos" left="525" width="1329" fontStyle="bold italic" top="9"/>
         </layout>
         <layout align="none" height="1660" width="664" top="52" left="5" frameStyle="/frames/FrameRetangular.xml" 
               margins="{left=30, top=15, right=10, bottom=10}">
               <label text="Equipamentos e pertences" width="624" left="260" fontStyle="bold italic" top="9"/>
               <layout align="top" height="30" top="25" margins="{bottom=4}">
                <button text="Adicionar Novo Item" width="150" top="45" align="left">
                        <event name="onClick">
                                -- Usuário clicou no botão de criar novo item.
                                -- Vamos inserir um novo item no nosso recordList                              
                                self.rclListaDosItens:append();
                        </event>
                </button>
        </layout>       

        <!-- recordList, representando a lista -->
        <recordList name="rclListaDosItens" field="campoDosItens" templateForm="frmItemDaLista" align="left" selectable="true" layout="vertical" top="40" height="80" width="150">                       

                <event name="onSelect">
                        --[[
                        Este evento é chamado quando o usuário selecionar/deselecionar itens da lista. Quando o usuário selecionar, vamos fazer nosso dataScopeBox (e todas as tag dentro dele) salvar e carregar dados no   objeto Nodo (NodeDatabase) do item selecionado.

                  --]]                      
                  local node = self.rclListaDosItens.selectedNode; 
                         self.boxDetalhesDoItem.node = node;                       
                  -- a caixa de detalhe só ficará visível se houver item selecionado
                          self.boxDetalhesDoItem.visible = (node ~= nil);
               </event>
        </recordList>                     

        <!-- dataScopeBox representando a nossa caixa de detalhes -->  

        <dataScopeBox name="boxDetalhesDoItem" visible="false" align="client"
                      margins="{left=4, right=4, top=2}">
                <!-- Faça um layout "bonito" para sua caixa de detalhes =). Utilize a propriedade "field" nas tags aqui dentro   -->      

                <rectangle align="none" color="black" xradius="10" yradius="10" height="180" width="480"
                           padding="{top=3, left=3, right=3, bottom=3}">                                   

                         <layout align="top" top="15" height="30" margins="{bottom=4}">
                                <label align="left" text="Titulo do Item:" autoSize="true"/>
                                <edit align="client" field="campoTitulo"/>
                                                               

                               <!-- Botão de apagar o item -->
                               <button align="right" text="Apagar!" 
                                      margins="{left=4, right=4, top=2, bottom=2}" width="150"
                                       onClick="NDB.deleteNode(self.boxDetalhesDoItem.node);" />

                        </layout>                       

                        <layout align="top" top="15" height="130">              
                                <image align="left" editable="true" field="campoURLImagem" width="130"/>                               

                                <layout align="client" margins="{left=2}">
                                        <edit align="top" field="campoSubTitulo" height="30"/>
                                        <textEditor align="top" field="campoTextoGrande" height="100"/>
                                </layout>                      
                        </layout>
                </rectangle>
        </dataScopeBox>

               
        </layout>
        <layout align="none" height="1660" width="664" top="52" left="669" frameStyle="/frames/FrameRetangular.xml" 
               margins="{left=30, top=15, right=10, bottom=10}">
               <label text="Dinheiro e Recursos" width="624" left="270" fontStyle="bold italic" top="9"/>
               
               <style>
                layout.LayoutCampo23 {
                        width: 15;
                        top: 25;
                        height: 25;
                        align: top;
                        margins: {left=2, right=2, top=5};
                }              

                layout.LayoutCampo23 edit {
                        height: 5;
                        width: 250;
                        fontSize: 15;
                        align: left;
                }              

                layout.LayoutCampo23 label {
                        align: left;
                        horzTextAlign: leading;
                        vertTextAlign: leading;
                        autoSize: true;
                        top: 30;

                }              
        </style>
        <layout left="5" top="25" width="664" height="1660">
               <layout class="LayoutCampo23" >
                        <label text="     Nivel de gastos:"/>  
                        <edit  field="dinheiro.ng"/>               
                </layout>
                <layout class="LayoutCampo23" >
                        <label text="     Dinheiro:"/>  
                        <edit  field="dinheiro.dinheiro"/>               
                </layout>
                <layout class="LayoutCampo23" >
                        <label text="     Patrimônio:"/>  
                        <edit  field="dinheiro.patrimonio"/>               
                </layout>

        </layout> 
        <textEditor left="20" top="125" width="624" height="1521" field="textEditor.patrimonio"/>
        </layout>
        </scrollBox>
        </tab>
