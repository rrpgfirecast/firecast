require("utils.lua");function trim(s)	if s ~= nil then		return (s:gsub("^%s*(.-)%s*$", "%1"));	else		return nil;	end;endfunction breakPathIntoParts(path)	local ret = {};		if type(path) == "string" then		for p in path:gmatch("[^/\\\13\10]+") do			p = trim(p);					if p ~= nil and p ~= "" then				ret[#ret + 1] = p;			end;		end;		end;		return ret;end;function includePathDelim(path)	local parts = breakPathIntoParts(path);	local r = "/";			for i = 1, #parts, 1 do		r = r .. parts[i] .. "/";	end;		return r;end;function fireDriveCollationPath(path)	local parts = breakPathIntoParts(path);	local ret = "";		for i = 1, #parts, 1 do		ret = ret .. "/" .. parts[i];	end;		return string.upper(utils.removerAcentos(ret));end;function extractFileName(path)	local parts = breakPathIntoParts(path);		if #parts > 0 then		return parts[#parts];	else		return "";	end;end;function extractFilePath(path)	local parts = breakPathIntoParts(path);	local ret = "/";		for i = 1, #parts - 1, 1 do		ret = ret .. parts[i] .. "/";	end;		return ret;end;